<p id="notice"><%= notice %></p>

<%= render 'card', :noise => @noise %>

<% if @noise.has_coordinates? %>
  <div class="card">
    <div data-map></div>
  </div>
<% end %>

<%= render :partial => 'admin/noises/card_addendum', :locals => {:noise => @noise} %>
<%= render :partial => 'admin/origins/card_addendum', :locals => {:noise => @noise} %>  


<% @noises.each do |noise| %>
  <%= render 'card', :noise => noise %>
  <%= render :partial => 'admin/noises/card_addendum', :locals => {:noise => noise} %>
  <%= render :partial => 'admin/origins/card_addendum', :locals => {:noise => noise} %>  
<% end %>