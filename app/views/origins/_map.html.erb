<div id="map-origin-<%= origin.id %>"
  data-map 
  data-map-bounds="<%= JSON(origin.map.boundaries) %>"
  >
  <div data-map-marker
    data-map-marker-latitude="<%= origin.latitude %>" 
    data-map-marker-longitude="<%= origin.longitude %>"
    data-map-marker-name="<%= origin.noise.user_name %>"
    data-map-marker-details="<%= origin.noise.text %>"
  >
  </div>
  <div data-map-panel id="map-panel-origin-<%= origin.id %>">
  </div>
</div>